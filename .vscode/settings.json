{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"eslint.validate": ["javascript", "vue"],
	"editor.tabSize": 2,
	"editor.insertSpaces": false,
	"files.eol": "\n",
	"search.exclude": {
		"**/.github": true,
		"**/dist": true,
		"**/node_modules": true,
		"**acronymWords.json": true,
		"pnpm-lock.yaml": true,
		"LICENSE": true,
		"README.md": true
	}
}
